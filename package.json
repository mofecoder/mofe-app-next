{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p 'dev:*'",
    "dev:client": "next dev -p 8000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "dev:pathpida": "pathpida --enableStatic --watch",
    "build": "run-p 'build:client' 'build:server'",
    "prebuild:client": "npm run generate",
    "build:client": "pathpida --enableStatic && aspida && next build && next export",
    "build:server": "npm run build --prefix server",
    "build:types": "pathpida --enableStatic && aspida && npm run build:frourio --prefix server",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:pathpida": "pathpida --enableStatic",
    "generate:server": "npm run generate --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.{ts,js,tsx}\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix && prettier --write \"./**/*.{ts,js,tsx}\"",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:dev:createonly": "npm run migrate:dev:createonly --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "seed": "npm run seed --prefix server",
    "start": "run-p 'start:*'",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "pretest": "npm run generate",
    "test": "jest",
    "pretypecheck": "npm run generate",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server"
  },
  "resolutions": {
    "@types/react": "^17",
    "@types/react-dom": "^17"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@aspida/react-query": "^1.11.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.9",
    "@prisma/client": "^3.15.2",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "firebase": "^9.17.1",
    "firebaseui": "^6.0.2",
    "frourio": "^0.31.1",
    "next": "^13.1.6",
    "pathpida": "^0.20.1",
    "prisma": "^3.15.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-query": "^3.39.3",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.58.1"
  },
  "devDependencies": {
    "@fastify/cors": "^8.2.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "fastify": "^4.13.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  }
}
